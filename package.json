{
    "name": "nginx-formatter",
    "displayName": "nginx-formatter",
    "description": "Formats Nginx Configuration",
    "version": "0.0.10",
    "publisher": "raynigon",
    "license": "SEE LICENSE IN LICENSE.md",
    "icon": "images/nginx.png",
    "galleryBanner": {
        "color": "#1289B9",
        "theme": "dark"
    },
    "keywords": [
        "nginx",
        "format",
        "formatter",
        "configuration"
    ],
    "engines": {
        "vscode": "^1.6.0"
    },
    "categories": [
        "Formatters"
    ],
    "activationEvents": [
        "onLanguage:NGINX",
        "onLanguage:NGINX Conf",
        "onLanguage:nginx"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/raynigon/vscode-nginx-formatter"
    },
    "main": "./out/src/extension",
    "contributes": {
        "languages": [
            {
                "id": "nginx",
                "aliases": [
                    "NGINX Conf",
                    "nginx"
                ],
                "extensions": [
                    "nginx.conf",
                    ".conf",
                    ".nginx"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "commands": []
    },
    "extensionDependencies": [
        "hangxingliu.vscode-nginx-conf-hint"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "tslint -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install && tsc",
        "package": "vsce package",
        "publish": "vsce publish"
    },
    "dependencies": {
        "nginxbeautifier": "1.0.19"
    },
    "devDependencies": {
        "@types/node": "14.0.23",
        "typescript": "4.0.3",
        "vsce": "^1.77.0",
        "vscode": "1.1.37"
    }
}
