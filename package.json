{
    "name": "nginx-formatter",
    "displayName": "nginx-formatter",
    "description": "Formats Nginx Configuration",
    "version": "0.0.2",
    "publisher": "raynigon",
    "license": "SEE LICENSE IN LICENSE.md",
    "icon": "images/nginx.png",
    "galleryBanner": {
        "color": "#1289B9",
        "theme": "dark"
    },
    "keywords": [
        "nginx",
        "format",
        "formatter",
        "configuration"
    ],
    "engines": {
        "vscode": "^1.6.0"
    },
    "categories": [
        "Formatters"
    ],
    "activationEvents": [
        "onLanguage:NGINX"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/raynigon/vscode-nginx-formatter"
    },
    "main": "./out/src/extension",
    "contributes": {
        "languages": [
            {
                "id": "NGINX",
                "aliases": [
                    "nginx"
                ],
                "filenamePatterns": [
                    "*.conf",
                    "NGINX"
                ]
            }
        ],
        "commands": []
    },
    "extensionDependencies":[
        "shanoor.vscode-nginx"
    ],
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install && tsc"
    },
    "dependencies": {
        "nginx-conf": ">=1.5.0"
    },
    "devDependencies": {
        "@types/node": "*",
        "vscode": ">=1.0.3",
        "typescript": ">=2.0.6"
    }
}
